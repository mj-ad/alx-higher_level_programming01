-- converts hbtn_0c_0 database to UTF8
ALTER DATABASE hbtn_0c_0 CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci;
ALTER DATABASE ALTER TABLE first_table CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci;
ALTER TABLE first_table CHANGE name name VARCHAR(20) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci;
