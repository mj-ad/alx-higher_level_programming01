#!/usr/bin/node
const process = require('process');
function add(a, b) {
    console.log(a + b);
}
const first = process.argv[2];
const second = process.argv[3];
if (isNaN(first) || typeof first === 'undefined') {
  console.log('NaN');
} else if (isNaN(second) || typeof second === 'undefined') {
  console.log('NaN');
} else {
  a = parseInt(first);
  b = parseInt(second);
    add(a, b);
}
